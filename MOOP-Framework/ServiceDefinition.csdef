<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="MOOP_Framework" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition">
  <WebRole name="MOOP-WebRole">
    <Sites>
      <Site name="Web">
        <Bindings>
          <Binding name="HttpIn" endpointName="HttpIn" />
          <Binding name="HttpsIn" endpointName="HttpsIn" />
        </Bindings>
      </Site>
    </Sites>
    <ConfigurationSettings>
      <Setting name="DiagnosticsConnectionString" />
      <Setting name="webRootContainer" />
      <Setting name="HandlerFbApps" />
      <Setting name="HandlerFbTransforms" />
      <Setting name="HandlerFbScripts" />
      <Setting name="allowInsecureRemoteEndpoints" />
      <Setting name="configBlobContainer" />
    </ConfigurationSettings>
    <LocalResources>
      <LocalStorage name="AzureLucene" cleanOnRoleRecycle="false" sizeInMB="512" />
    </LocalResources>
    <Certificates>
      <Certificate name="AzureArchitect" storeLocation="LocalMachine" storeName="My" />
    </Certificates>
    <Endpoints>
      <InputEndpoint name="HttpIn" protocol="http" port="80" />
      <InputEndpoint name="HttpsIn" protocol="https" port="443" certificate="AzureArchitect" />
    </Endpoints>
    <Imports>
    </Imports>
  </WebRole>
  <WorkerRole name="MOOPWorkerRole">
    <ConfigurationSettings>
      <Setting name="DiagnosticsConnectionString" />
      <Setting name="BlobStorageEndpoint" />
      <Setting name="configBlobContainer" />
      <Setting name="SleepDuration" />
      <Setting name="queueName" />
    </ConfigurationSettings>
    <Certificates>
      <Certificate name="AzureArchitect" storeLocation="LocalMachine" storeName="My" />
    </Certificates>
    <Imports>
    </Imports>
  </WorkerRole>
</ServiceDefinition>